<resources>
    <declare-styleable name="Morph">
        <attr name="startColor" format="color|reference" />
        <attr name="endColor" format="color|reference" />
        <attr name="endCornerRadius" format="dimension|reference" />
        <attr name="startCornerRadius" format="dimension|reference" />
        <attr name="opticalButtonColor" format="color|reference" />
        <attr name="fabIcon" format="reference" />
        <attr name="fabIconTint" format="reference|color" />
        <attr name="holdAtEnd" format="reference|boolean" />
    </declare-styleable>
    <declare-styleable name="OptionItemView">
        <attr name="optionTitle" format="string|reference" />
        <attr name="optionDes" format="string|reference" />
        <attr name="optionIcon" format="reference" />
        <attr name="optionShowSwitch" format="reference|boolean" />
        <attr name="optionShowTopDivider" format="reference|boolean" />
    </declare-styleable>
    <declare-styleable name="BottomBar">
        <attr name="imageEntries" format="reference" />
        <attr name="captionEntries" format="reference" />
    </declare-styleable>
    <declare-styleable name="ElasticDragDismissFrameLayout">
        <attr name="dragDismissTarget" format="reference" />
        <attr name="dragDismissDistance" format="dimension" />
        <attr name="dragDismissFraction" format="float" />
        <attr name="dragDismissScale" format="float" />
        <attr name="dragElasticity" format="float" />
    </declare-styleable>
    <declare-styleable name="LineCounter">
        <attr name="lineCounterTextColor" format="reference|color" />
        <attr name="lineCounterTextSize" format="reference|dimension" />
    </declare-styleable>
    <declare-styleable name="ChartBarView">
        <attr name="barColor" format="color|reference" />
    </declare-styleable>
</resources>